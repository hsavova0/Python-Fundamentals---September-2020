age = int(input())

if age == 88:
    print("Leo finally won the Oscar! Leo is happy")
elif age == 86:
    print("Not even for Wolf of Wall Street?!")
elif 86 < age < 88:
    print("When will you give Leo an Oscar?")
elif age < 86:
    print("When will you give Leo an Oscar?")
else:
    print("Leo got one already!")
