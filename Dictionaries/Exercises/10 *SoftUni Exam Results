data = input().split("-")
results = {}
languages = {}
counter_c = 0
counter_j = 0

while "exam finished" not in data:
    for word in data:
        if "banned" not in data:
            username = data[0]
            language = data[1]
            points = data[2]
            results[username] = points
            languages[username] = language
        else:
            username = data[0]
            if username in results:
                results.pop(username)
    data = input().split("-")

for name, language in languages.items():
    if language == "C#":
        counter_c += 1
    elif language == "Java":
        counter_j += 1

print("Results:")
for name, points in results.items():
    print(f"{name} | {points}")

print("Submissions:")
print(f"C# - {counter_c}")
print(f"Java - {counter_j}")

